{
  "name": "commitit",
  "publisher": "HaydenLiles",
  "displayName": "Commit It",
  "description": "",
  "repository": "https://github.com/Hayden-Liles/autocommit",
  "version": "0.0.6",
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onFileSystem:git"
  ],
  "contributes": {
    "configuration": {
      "title": "AutoCommit",
      "properties": {
        "AutoCommit.apiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API Key"
        },
        "AutoCommit.autoSync": {
          "type": "boolean",
          "default": true,
          "description": "Auto Sync after commit"
        }
      }
    },
    "commands": [
      {
        "command": "commitAll",
        "title": "commitAll",
        "icon": "./auto-upload.svg"
      }
    ],
    "menus": {
      "scm/title": [
        {
          "command": "commitAll",
          "when": "scmProvider == git",
          "group": "navigation"
        }
      ]
    },
    "views": {
      "scm": [
        {
          "id": "unpushedCommits",
          "name": "Unpushed Commits"
        }
      ]
    }
  },
  "main": "./extension.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "openai": "^3.3.0"
  }
}
